{
  "kind": "collectionType",
  "collectionName": "labs",
  "info": {
    "singularName": "lab",
    "pluralName": "labs",
    "displayName": "Lab"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "LabType": {
      "type": "enumeration",
      "required": true,
      "default": "Research",
      "enum": [
        "Research",
        "Teaching"
      ]
    },
    "LabLogo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "LabName": {
      "type": "string",
      "required": true
    },
    "ShortDescription": {
      "type": "string",
      "required": true
    },
    "LongDescription": {
      "type": "text",
      "required": true
    },
    "Faculty": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::faculty.faculty"
    },
    "WebsiteLink": {
      "type": "component",
      "component": "components.link",
      "repeatable": false
    },
    "AccentColor": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true
    }
  }
}
